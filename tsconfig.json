{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "api/*": ["./src/api/*"],
      "assets/*": ["./src/assets/*"],
      "common/*": ["./src/common/*"],
      "components/*": ["./src/components/*"],
      "helpers/*": ["./src/helpers/*"],
      "interfaces/*": ["./src/interfaces/*"],
      "navigation/*": ["./src/navigation/*"],
      "screens/*": ["./src/screens/*"],
      "store/*": ["./src/store/*"],
      "@/*": ["./src/*"]
    }
  }
}
